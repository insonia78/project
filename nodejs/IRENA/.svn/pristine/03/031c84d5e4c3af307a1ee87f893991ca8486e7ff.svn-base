google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(energy_storage_MW1);

function energy_storage_MW1() {

  var data_MW1= google.visualization.arrayToDataTable([
    ['C rate', 'MW'],
    ['2C',     509],
    ['1C',      136],
    ['0.5C',  70],
    ['0.25C', 276]
    ]);

    var options_MW1 = {
    title: 'MW Size',
    titleTextStyle:     {fontSize: '14'},
    pieSliceText: 'value',
    legend: {position: 'bottom'}
    };

    var chart = new google.visualization.PieChart(document.getElementById('piechart_MW1'));

    chart.draw(data_MW1, options_MW1);
    }


