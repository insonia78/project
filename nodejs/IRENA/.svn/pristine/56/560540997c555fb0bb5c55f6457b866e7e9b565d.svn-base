/*style the selects on the edit bar*/
 .select-editable {
     position:relative;
     background-color:white;
     border:solid grey 1px;
     width:120px;
     height:36px;
 }
 .select-editable select {
     position:absolute;
     top:0px;
     left:0px;
     font-size:14px;
     border:none;
     width:120px;
     margin:0;
 }
 .select-editable input {
     position:absolute;
     top:0px;
     left:0px;
     width:100px;
     padding:0px;
     font-size:12px;
     border:none;
     height:36px;
     text-align: center;
     vertical-align: middle;
     font-weight:bold;
    
 }
 .select-editable select:focus, .select-editable input:focus {
     outline:none;
 }

 .input-group{
   margin-top:5px;
 }

 #side-menu li, #Inputs a, #Outputs a { cursor: pointer; cursor: hand; }

h1, a, li, h2 {
  color:#00609C;
}

li > a:hover{
  color:#00BFFF;
}
thead > tr >th{
  background-color:#00BFFF;
  color:#FFFFFF;
  vertical-align:top !important;
  border: 1px solid #00BFFF !important;
}

h2, h3{
  margin-bottom:20px;
}

