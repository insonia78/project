
.col-lg-8
  h3 Demand Side  
  table#policies1-tab.table.table-striped.table-bordered.table-hover
    thead
      tr
        th.col-lg-2 
        |
        th Power(MW)
        |
        th Energy(MWh)
        |
        th Cost ($/MWh)
           
    tbody  
      tr
        th Energy Efficiency
        |
        td 
          input.form-control#eneffpower(type='text' name='eneffpower')
        |
        td 
          input.form-control#eneffeng(type='text' name='eneffeng')
        |
        td 
          input.form-control#eneffcost(type='text' name='eneffcost')
      tr
        th Demand Response
        |
        td 
          input.form-control#demrespower(type='text' name='demrespower')
        |
        td 
          input.form-control#demreseng(type='text' name='demreseng')
        |
        td 
          input.form-control#demrescost(type='text' name='demrescost')
      tr
        th Distributed PV
        |
        td 
          input.form-control#distpvpower(type='text' name='distpvpower')
        |
        td 
          input.form-control#distpveng(type='text' name='distpveng')
        |
        td 
          input.form-control#distpvcost(type='text' name='distpvcost')

.col-lg-8 
  h3 Renewables 
  table#policies2-tab.table.table-striped.table-bordered.table-hover
    thead
      tr
        th.col-lg-3 
        |
        th Capacity Year 1 (MW)
        |
        th Capacity Year 2 (MW)
        |
        th Cost ($/MWh)

    tbody  
      tr
        th Wind
        |
        td 
          input.form-control#windcap1(type='text' name='windcap1' value='1361')
        |
        td
          input.form-control#windcap2(type='text' name='windcap2')
        |
        td 
          input.form-control#windcost(type='text' name='windcost')

      tr
        th Solar
        |
        td 
          input.form-control#solarcap1(type='text' name='solarcap1')
        |
        td
          input.form-control#solarcap2(type='text' name='solarcap2')
        |
        td 
          input.form-control#solarcost(type='text' name='solarcost')
