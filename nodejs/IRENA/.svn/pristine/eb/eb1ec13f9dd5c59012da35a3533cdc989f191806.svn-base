google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(without_storage1);

function without_storage1() {

  var data_wos1= google.visualization.arrayToDataTable([
    ['Fuel', 'amount'],
    ['Coal',     39724],
    ['Hydro',      1895],
    ['NG',  32886],
    ['Wind', 3749],
    ['others', 2004]
    ]);

    var options_wos1 = {
    title: 'Without Storage',
    titleTextStyle:     {fontSize: '14'},
    pieSliceText: 'value',
    legend: {position: 'bottom'}
    };

    var chart = new google.visualization.PieChart(document.getElementById('without_storage1'));

    chart.draw(data_wos1, options_wos1);
    }


