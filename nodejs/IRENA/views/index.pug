extends layout

block content
   div(id='inputs' class='io-container')
     include input-form2.pug
   div(id='settings' class='io-container' style='display:none;')
     include settings-form
   div(id='outputs' class='io-container' style="display:none;")
     include output-forms.pug
     include output-graphs.pug
   div(id='outputs_noess' class='io-container' style="display:none;")
     include output-forms-noess.pug
     include output-graphs-noess.pug
   div(id='stacked_benefits' class='io-container' style='display:none;')
     include stacked-benefits-form.pug
   div(id='tabular' class='io-container' style='display:none;')
     include tabular-form.pug

   #myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLongTitle', aria-hidden='true')
      .modal-dialog.modal-lg(role='document')
        .modal-content
          .modal-header
            h4#myModalTitle 
          //-
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') Ã—
          .modal-body

            #modalBody                    
              #data-dictionary(style='display:none')
                include sub-forms/data-dictionary.pug

              #user-guide(style='display:none')
                include sub-forms/user-guide.pug

          .modal-footer
            p Acelerex, 2018
    
   script(src='../bower_components/jquery-validation/dist/jquery.validate.min.js')
   script(src='../js/index.js')
   script(type='text/javascript', src='../js/pie-gen-by-fuel.js')
   script(type='text/javascript', src='../js/output-charts.js')
